<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="eu.ydp.gwtutil.client.components.exlistbox.ExListBoxStyleNames" field="styleNames" />
	<g:FocusPanel styleName="{styleNames.popupOptionPanel.toString}" ui:field="popupPanel">
		<g:FlowPanel styleName="{styleNames.popupOptionPanelInner.toString}" ui:field="popupContainer">
			<g:FocusPanel styleName="{styleNames.popupOptionButton.toString}" ui:field="popupButton" />
		</g:FlowPanel>
	</g:FocusPanel>
</ui:UiBinder> 